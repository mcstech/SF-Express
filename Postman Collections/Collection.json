  "item": [
    {
      "name": "Order",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "id": "102409cd-f4bc-4cc3-9cde-ab3d14dc5779",
            "exec": [
              "var appKey = pm.collectionVariables.get(\"appKey\");",
              "var aesKey = pm.collectionVariables.get(\"aesKey\");",
              "var token = pm.collectionVariables.get(\"token\");",
              "var time = (new Date()).getTime();",
              "",
              "pm.collectionVariables.set(\"timestamp\", time);",
              "",
              "var nonce = pm.collectionVariables.get(\"nonce\");",
              "var customerCode = pm.collectionVariables.get(\"customerCode\");",
              "var data = {",
              "    customerCode: customerCode,",
              "    sfWaybillNo: 'SF1324481972744'",
              "};",
              "var ivWA = CryptoJS.lib.WordArray.random(16);",
              "var strData = JSON.stringify(data);",
              "var dataWA = CryptoJS.enc.Utf8.parse(strData);",
              "var keyWA = CryptoJS.enc.Base64.parse(aesKey);",
              "var ciphertext = CryptoJS.AES.encrypt(dataWA, keyWA, {iv: ivWA}).ciphertext;",
              "var ivCiphertext = ivWA.clone().concat(ciphertext);",
              "var ivCiphertextB64 = ivCiphertext.toString(CryptoJS.enc.Base64);",
              "",
              "console.log('encryptedData:', ivCiphertextB64);",
              "",
              "pm.collectionVariables.set('request_body', ivCiphertextB64);",
              "",
              "var toBeEncrypt = token + time + nonce + strData;",
              "var signature = CryptoJS.SHA256(toBeEncrypt).toString();",
              "",
              "pm.collectionVariables.set(\"signature\", signature);",
              "",
              "console.log('token:', token);",
              "console.log('timestamp:', time);",
              "console.log('nonce', nonce);",
              "console.log('customerCode:', customerCode);",
              "console.log('signature:', signature);"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "id": "ef01cbc0-d999-4b80-ad81-bea4e181be83",
